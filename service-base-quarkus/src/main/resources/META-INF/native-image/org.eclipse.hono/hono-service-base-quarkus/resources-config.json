{
  "resources": {
    "includes": [
      {
        "pattern": ".*\\.xml$"
      },
      {
        "pattern": ".*\\.json$"
      },
      {
        "pattern": ".*\\.yaml$"
      },
      {
        "pattern": ".*\\.properties$"
      },
      {
        "pattern": ".*\\.proto$"
      },
      {
        "pattern": "META-INF\\/services\\/io\\.jsonwebtoken\\..*$"
      }
    ],
    "excludes": [
      {
        "pattern": "META-INF\\/maven\\/.*$"
      },{
        "pattern": "META-INF\\/native-image\\/.*$"
      }
    ]
  }
}
